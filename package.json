{
  "name": "quark-signal",
  "version": "0.0.0-development",
  "description": "Quark signal",
  "main": "lib/index.js",
  "scripts": {
    "build": "BABEL_ENV=production babel src -d lib",
    "docs": "node_modules/.bin/jsdoc --configure jsdoc.json --verbose",
    "docs:deploy": "gh-pages -d docs/",
    "test": "ava",
    "test:watch": "ava --watch --verbose",
    "test:coverage": "nyc npm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "npm test && npm run build"
  },
  "keywords": [
    "quark",
    "signal",
    "signals",
    "promise",
    "events"
  ],
  "author": "fm_ph",
  "contributors": [
    "Patrick Heng <hengpatrick.pro@gmail.com> (http://hengpatrick.fr/)",
    "Fabien Motte <contact@fabienmotte.com> (http://fabienmotte.com/)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fm-ph/quark-signal.git"
  },
  "bugs": {
    "url": "https://github.com/fm-ph/quark-signal/issues"
  },
  "homepage": "https://github.com/fm-ph/quark-signal",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "gh-pages": "^0.12.0",
    "jsdoc": "^3.4.3",
    "minami": "nijikokun/minami",
    "nyc": "^10.1.2",
    "semantic-release": "^6.3.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime",
      "transform-object-rest-spread"
    ],
    "ignore": "test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-core/register"
    ]
  }
}
